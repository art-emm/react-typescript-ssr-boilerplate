{
  "name": "webpack-ssr-ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && NODE_ENV=development npm run watch:server",
    "webpack": "TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack",
    "build": "npm run clean && npm run build:server && npm run build:client && npm run build:serverRenderer",
    "build:client": "webpack --config ./webpack.client.prod.ts -p --mode=production",
    "build:serverRenderer": "webpack --config ./webpack.server.ts -p --mode=production",
    "build:server": "tsc -p tsconfig-webpack.json",
    "watch:server": "nodemon",
    "start": "NODE_ENV=production node build/server/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/extract-text-webpack-plugin": "^3.0.1",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-loadable": "^5.3.3",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/webpack": "^4.0.0",
    "@types/webpack-dev-middleware": "^2.0.0",
    "@types/webpack-hot-middleware": "^2.16.2",
    "@types/webpack-node-externals": "^1.6.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^1.17.1",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.4.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-stats-plugin": "^0.2.1"
  },
  "dependencies": {
    "@types/copy-webpack-plugin": "^4.4.0",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
